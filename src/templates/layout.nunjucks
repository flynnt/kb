<!doctype html>
<!--
    Build information
    Version: {{version}}
    Date: {{date}}
    Environment: {{env}}
-->
<html class="no-js" lang="en-us">
<head>
    <title>Keri B. Burchfield, PhD.</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <meta name="theme-color" content="#000000" />
    <!--[if IE]><meta http-equiv="cleartype" content="on" /><![endif]-->

    <!-- Touch Icons - iOS and Android -->
    <link rel="apple-touch-icon" sizes="57x57" href="/kb/assets/media/images/apple-touch-icon-57x57.png?={{version}}" />
    <link rel="apple-touch-icon" sizes="60x60" href="/kb/assets/media/images/apple-touch-icon-60x60.png?={{version}}" />
    <link rel="apple-touch-icon" sizes="72x72" href="/kb/assets/media/images/apple-touch-icon-72x72.png?v={{version}}" />
    <link rel="apple-touch-icon" sizes="76x76" href="/kb/assets/media/images/apple-touch-icon-76x76.png?v={{version}}" />
    <link rel="apple-touch-icon" sizes="114x114" href="/kb/assets/media/images/apple-touch-icon-114x114.png?v={{version}}" />
    <link rel="apple-touch-icon" sizes="120x120" href="/kb/assets/media/images/apple-touch-icon-120x120.png?v={{version}}" />
    <link rel="apple-touch-icon" sizes="144x144" href="/kb/assets/media/images/apple-touch-icon-144x144.png?v={{version}}" />
    <link rel="apple-touch-icon" sizes="152x152" href="/kb/assets/media/images/apple-touch-icon-152x152.png?v={{version}}" />
    <link rel="apple-touch-icon" sizes="180x180" href="/kb/assets/media/images/apple-touch-icon-180x180.png?v={{version}}" />
    <link rel="apple-touch-icon" sizes="196x196" href="/kb/assets/media/images/apple-touch-icon-196x196.png?v={{version}}" />

    <!-- For IE 9 and below. ICO should be 32x32 pixels in size generated from http://www.xiconeditor.com/ or might not work  -->
    <!--[if lte IE 9]><link rel="shortcut icon" href="/kb/assets/media/images/favicon-32x32.ico?v={{version}}" type="image/x-icon" ><![endif]-->

    <!-- All other browsers -->
    <link rel="icon" type="image/png" sizes="196x196" href="/kb/assets/media/images/favicon-196x196.png?v={{version}}" />
    <link rel="icon" type="image/png" sizes="192x192" href="/kb/assets/media/images/favicon-192x192.png?v={{version}}" />
    <link rel="icon" type="image/png" sizes="160x160" href="/kb/assets/media/images/favicon-160x160.png?v={{version}}" />
    <link rel="icon" type="image/png" sizes="96x96" href="/kb/assets/media/images/favicon-96x96.png?v={{version}}" />
    <link rel="icon" type="image/png" sizes="32x32" href="/kb/assets/media/images/favicon-32x32.png?v={{version}}" />
    <link rel="icon" type="image/png" sizes="16x16" href="/kb/assets/media/images/favicon-16x16.png?v={{version}}" />

    <!-- IE 11+ on Windows 8.1+ -->
    <meta name="msapplication-TileImage" content="/kb/assets/media/images/mstile-144x144.png?v={{version}}">
    <meta name="msapplication-TileColor" content="#ffffff">

    {% block styles %}
        <link href="https://fonts.googleapis.com/css?family=Taviraj:400,400i,700,700i|Work+Sans:400,600,700" rel="stylesheet">
    <link rel="stylesheet" media="all" href="/kb/assets/styles/main.css?v={{version}}" />
    {% endblock %}
</head>

<body class="site">
    <header class="site-hd">
        <div class="wrapper">
            <div class="masthead">
                <div class="branding">
                    {% include './partials/_birb.nunjucks' %}
                </div>
                <h1 class="hdg hdg--heraldic">Keri B. Burchfield, PhD</h1>
            </div>
        </div>
    </header>
    <nav class="site-nav">
        <div class="wrapper wrapper--constrained">
            <div class="hideAboveSmall">
                <label class="isVisuallyHidden" for="smallNav">Site Navigation</label>
                <select class="field field--select" name="smallNav" id="smallNav">
                    <option value="">Navigate to &hellip;</option>
                    <option value="/kb/">Home</option>
                    <option value="/kb/research.html">Research</option>
                    <option value="/kb/teaching.html">Teaching</option>
                    <option value="/kb/vitae.html">Vitae</option>
                    <option value="/kb/contact.html">Contact</option>
                </select>
            </div>
            <div class="showAboveSmall">
                <ul class="hList hList--2x">
                    <li>
                        <a class="txt txt--action" href="/">Home</a>
                    </li>
                    <li>
                        <a class="txt txt--action" href="/kb/research.html">Research</a>
                    </li>
                    <li>
                        <a class="txt txt--action" href="/kb/teaching.html">Teaching</a>
                    </li>
                    <li>
                        <a class="txt txt--action" href="/kb/vitae.html">Vitae</a>
                    </li>
                    <li>
                        <a class="txt txt--action" href="/kb/contact.html">Contact</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <main class="site-content">
        <div class="wrapper wrapper--constrained">
            {% block content %}
            {% endblock %}
        </div>
    </main>
    <footer role="contentinfo" class="site-ft">
        <span class="txt txt--diminutive">
            &copy; 2018 Keri Burchfield, PhD
        </span>
    </footer>
    {% block scriptsBody %}
        <script>
            var jumpy = document.getElementById('smallNav');
            jumpy.addEventListener('change', function(e) {
                window.location = e.currentTarget.value;
            });
        </script>
    {% endblock %}
</body>
</html>
